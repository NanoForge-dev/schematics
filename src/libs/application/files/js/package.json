{
  "name": "<%= name %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "author": "<%= author %>",
  "scripts": {
    "build": "nf build",
    "start": "nf start",
    "lint": "prettier --check . && eslint --format=pretty \"{client,server}/**/*.js\"",
    "format": "prettier --write . && eslint --fix --format=pretty \"{client,server}/**/*.js\""
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.39.0",
    "@nanoforge-dev/cli": "^0.0.1",
    "eslint": "^9.39.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2"
  },
  "private": true,
  "lint-staged": {
    "**/*.js": [
      "prettier --write"
    ],
    "{client,server}/**/*.js": [
      "eslint --fix"
    ]
  }
}
